{
  "name": "angular-signal-form",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:libs",
    "start": "ng serve",
    "build": "ng build",
    "build:libs:users-managment": "ng build users-managment",
    "build:libs:shared-lib": "ng build shared-lib",
    "build:libs": "npm run build:libs:shared-lib && npm run build:libs:users-managment",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,html,css,scss}\" \"projects/**/*.{ts,html,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,scss}\" \"projects/**/*.{ts,html,css,scss}\""
  },
  "private": true,
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@tailwindcss/postcss": "^4.1.12",
    "angular-eslint": "^21.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.50.1",
    "vitest": "^4.0.8"
  }
}
